{
  "plugins": [],
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    ["@babel/preset-typescript"]
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-react",
          {
            "runtime": "automatic",
